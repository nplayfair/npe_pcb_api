{
  "development": {
    "dialect": "postgres",
    "use_env_variable": "DATABASE_URL"
  },
  "test": {
    "dialect": "sqlite",
    "storage": "./database.sqlite3",
    "logging": false
  },
  "production": {
    "dialect": "postgres",
    "ssl": true,
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    },
    "use_env_variable": "DATABASE_URL"

  }
}
